<div>
  <ol class="dialog-options">
    {{#each choices as |choice key|}}
    <li>
      <label class="checkbox">
        <input type="radio" name="choices" id="{{key}}" data-dtype="Boolean" {{checked choice.chosen}}>
        {{localize choice.label}}
      </label>
    </li>
    {{/each}}
  </ol>
</div>

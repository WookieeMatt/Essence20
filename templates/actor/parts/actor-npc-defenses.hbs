<div class="npc-defenses-section">
  <div class="npc-defenses-container">
    <div class="npc-defenses">
      {{#each system.defenses as |fields defense|}}
      <div class="resource flexcol">
        <label class="resource flex-group-center">{{localize (concat 'E20.defenses.' defense)}}</label>
        <input type="number" style="text-align: center;" name="system.defenses.{{defense}}.value" value="{{fields.value}}" />
      </div>
      {{/each}}
    </div>
  </div>
</div>

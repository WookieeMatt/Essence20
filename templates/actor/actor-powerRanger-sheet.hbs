<form class="{{cssClass}} {{actor.type}} flexcol" autocomplete="off">

  {{!-- <div class="pr-outer-border">
    <div class="pr-inner-border"> --}}

      {{!-- Actor Sheet Header --}}
      {{> "systems/essence20/templates/actor/parts/actor-pr-header.hbs"}}

      <nav class="sheet-tabs tabs" data-group="primary">
        <a class="item" data-tab="skills"> {{ localize "E20.skills" }}</a>
        <a class="item" data-tab="combat">{{ localize "E20.combat" }}</a>
        <a class="item" data-tab="gear">{{ localize "E20.gear" }}</a>
        <a class="item" data-tab="powers">{{ localize "E20.powers" }}</a>
        <a class="item" data-tab="perks">{{ localize "E20.perks" }}</a>
        <a class="item" data-tab="background">{{ localize "E20.background" }}</a>
        <a class="item" data-tab="notes">{{ localize "E20.notes" }}</a>
      </nav>

      {{!-- Actor Sheet Body --}}
      <section class="sheet-body">
        {{!-- Skills Tab --}}
        <div class="tab skills" data-group="primary" data-tab="skills">
          {{> "systems/essence20/templates/actor/parts/actor-common.hbs"}}
        </div>

        {{!-- Combat Tab --}}
        <div class="tab combat" data-group="primary" data-tab="combat">
          {{> "systems/essence20/templates/actor/parts/actor-defenses.hbs"}}
          {{> "systems/essence20/templates/actor/parts/actor-weapons.hbs"}}
          {{> "systems/essence20/templates/actor/parts/actor-armor.hbs"}}
        </div>

        {{!-- Gear Tab --}}
        <div class="tab gear" data-group="primary" data-tab="gear">
          {{> "systems/essence20/templates/actor/parts/actor-gear.hbs"}}
          {{> "systems/essence20/templates/actor/parts/actor-weapons.hbs"}}
          {{> "systems/essence20/templates/actor/parts/actor-armor.hbs"}}
        </div>

        {{!-- Powers Tab --}}
        <div class="tab powers" data-group="primary" data-tab="powers">
          {{> "systems/essence20/templates/actor/parts/actor-powers.hbs"}}
        </div>

        {{!-- Perks Tab --}}
        <div class="tab perks" data-group="primary" data-tab="perks">
          {{> "systems/essence20/templates/actor/parts/actor-perks.hbs"}}
        </div>

        {{!-- Origin Tab --}}
        <div class="tab background" data-group="primary" data-tab="background">
          {{> "systems/essence20/templates/actor/parts/actor-background.hbs"}}
        </div>

        {{!-- Notes Tab --}}
        <div class="tab notes" data-group="primary" data-tab="notes">
          {{> "systems/essence20/templates/actor/parts/actor-notes.hbs"}}
        </div>

      </section>
    {{!-- </div>
  </div> --}}
</form>

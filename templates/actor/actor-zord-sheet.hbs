<form class="{{cssClass}} {{actor.type}} flexcol" autocomplete="off">
  <header>
    {{!-- Actor Sheet Header --}}
    {{> "systems/essence20/templates/actor/parts/actor-base-header.hbs"}}
    <div class="zord-header">
      <input type="text" name="system.prerequisite" value="{{system.prerequisite}}" placeholder="{{ localize 'E20.prerequisite' }}" />
      <input type="text" name="system.ranger" value="{{system.ranger}}" placeholder="{{ localize 'E20.ranger' }}" />
      <input type="checkbox" name="system.isCombiner" {{checked system.isCombiner}} />
    </div>
    {{!-- Actor Sheet Body --}}
    <nav class="sheet-tabs tabs" data-group="primary">
      <a class="item" data-tab="zord"> {{ localize "E20.zord" }}</a>
      <a class="item" data-tab="notes">{{ localize "E20.notes" }}</a>
    </nav>
  </header>
  <section class="sheet-body">
    {{!-- Skills Tab --}}
    <div class="tab zord" data-group="primary" data-tab="skills">
      {{> "systems/essence20/templates/actor/parts/actor-zord-tab.hbs"}}
      {{> "systems/essence20/templates/actor/parts/actor-common.hbs"}}
    </div>

    {{!-- Notes Tab --}}
    <div class="tab notes" data-group="primary" data-tab="notes">
      {{> "systems/essence20/templates/actor/parts/actor-notes.hbs"}}
    </div>
  </section>
</form>

<form autocomplete="off" onsubmit="event.preventDefault();">
  {{#*inline "traitList"}}
  <ol class="dialog-options">
    {{#each choices as |choice key|}}
    <li>
      <label class="checkbox">
        <input type="checkbox" id="{{key}}" name="{{key}}" data-dtype="Boolean" {{checked choice.chosen}}>
        {{localize choice.label}}
      </label>
      {{#if choice.children}}
      {{> traitList choices=choice.children}}
      {{/if}}
    </li>
    {{/each}}
  </ol>
  {{/inline}}

  {{> traitList}}

</form>
